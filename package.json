{
  "name": "react-awesome-chat",
  "version": "1.0.6",
  "scripts": {
    "start": "dumi dev",
    "docs:build": "dumi build",
    "docs:deploy": "gh-pages -d docs-dist",
    "build": "father-build && node ./scripts/moveDeclare.js && node ./scripts/changeLess2Css.js",
    "deploy": "npm run docs:build && npm run docs:deploy",
    "release": "npm run build && npm publish",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\"",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "main": "lib/index.js",
  "module": "es/index.js",
  "typings": "lib/index.d.ts",
  "files": [
    "dist",
    "lib",
    "es"
  ],
  "author": "draco-china",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/draco-china/react-awesome-chat.git"
  },
  "keywords": [
    "react",
    "chat",
    "UI",
    "聊天"
  ],
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@babel/runtime": "^7.14.6",
    "classnames": "^2.3.1",
    "moment": "^2.29.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "uuid": "^3.4.0"
  },
  "devDependencies": {
    "@types/md5": "^2.3.0",
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.8",
    "@types/uuid": "^8.3.0",
    "@umijs/test": "^3.0.5",
    "babel-plugin-import": "^1.13.3",
    "dumi": "^1.0.14",
    "father-build": "^1.19.6",
    "fs-extra": "^10.0.0",
    "gh-pages": "^3.0.0",
    "install": "^0.13.0",
    "klaw-sync": "^6.0.0",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.1",
    "yorkie": "^2.0.0"
  }
}
